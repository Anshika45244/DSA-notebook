//linear search
#include<iostream>
using namespace std;
bool search(int arr[],int size, int key){
    for(int i = 0; i<size; i++){
        if(arr[i]==key){
            return true;
            
        }
    }
    return false;
}
int main(){
    int arr[10]={5,6,8,4,10,12,32,54};
    cout<<"enter the element to search"<<endl;
    int key;
    cin>>key;
    bool found = search(arr,10,key);
    if(found){
        cout<<"key is present"<<endl;
        
    }
    else{
        cout<<"key is absent"<<endl;
    }
    return 0;
}
